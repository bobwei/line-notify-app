{
  "name": "react-app-boilerplate",
  "author": "Bob Wei <bob61016@gmail.com>",
  "description": "A base skeleton template for react and webpack",
  "license": "MIT",
  "engines": {
    "node": "^6.9.0"
  },
  "scripts": {
    "start": "npm run clean && webpack --config webpack/webpack.config.server.babel.js && ENABLE_EXTRACT_TEXT_PLUGIN=false node server/index.js",
    "build": "npm run clean && npm run copy && NODE_ENV=production webpack -p --config webpack/webpack.config.server.babel.js && NODE_ENV=production webpack -p --config webpack/webpack.config.prod.babel.js && ./scripts/build.jsx > build/client/index.html",
    "build:parallel": "npm run clean && npm run copy && NODE_ENV=production parallel-webpack -p --config webpack/webpack.config.all.babel.js && ./scripts/build.jsx > build/client/index.html",
    "server": "NODE_ENV=production node server/index.js",
    "copy": "copyfiles -f ./src/favicon.ico ./build/client",
    "clean": "rimraf build/*",
    "deploy:gh-pages": "npm run build && gh-pages -d build/client",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.2.0",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "glob": "^7.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "minimist": "^1.2.0",
    "node-sass": "^3.13.0",
    "null-loader": "^0.1.1",
    "parallel-webpack": "^1.5.0",
    "postcss": "^5.0.21",
    "postcss-loader": "^1.1.1",
    "proxy-middleware": "^0.15.0",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "1.3.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.6",
    "webpack": "^v2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "bootstrap-sass": "^3.3.6",
    "compression": "^1.6.2",
    "core-js": "^2.0.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "fbjs": "^0.8.6",
    "font-awesome": "^4.7.0",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
